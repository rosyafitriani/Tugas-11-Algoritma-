public class CetakMundur {

    public static int reverse(int n) {
        System.out.print(n); 
        if (n > 1) {
            System.out.print(" , "); 
            return reverse(n - 1); 
        } else {
            return 1; 
        }
    }

    public static void main(String[] args) {
        int n = 5;
        System.out.print(" ");
        reverse(n); 
    }
}
